{
  "buildCommand": "cd frontend && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "cacheDirectories": [
    "node_modules",
    "frontend/node_modules",
    ".next/cache"
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
} 